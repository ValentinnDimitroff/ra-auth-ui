{
    "name": "ra-auth-ui",
    "version": "1.0.8",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "dependencies": {
        "prop-types": "^15.7.2",
        "vite": "^5.0.12"
    },
    "peerDependencies": {
        "@mui/icons-material": "^5.0.1",
        "@mui/material": "^5.0.2",
        "react": "^16.9.0 || ^17.0.0 || ^18.0.0",
        "react-admin": "^4.x",
        "react-dom": "^16.9.0 || ^17.0.0 || ^18.0.0",
        "react-router": "^6.1.0",
        "react-router-dom": "^6.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.5",
        "@mui/icons-material": "^5.15.2",
        "@mui/material": "^5.15.2",
        "@testing-library/jest-dom": "^5.11.5",
        "@testing-library/react": "^11.1.1",
        "@vitejs/plugin-react": "^4.2.1",
        "babel-jest": "^26.6.3",
        "eslint": "^7.18.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "history": "^5.3.0",
        "jest": "^26.6.3",
        "prettier": "2.2.1",
        "react-admin": "^4.16.4",
        "react-hook-form": "^7.49.2",
        "react-router-dom": "^6.1.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.3.3"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/examples/demo/"
        ],
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$",
            "<root_dir>/src/"
        ]
    },
    "scripts": {
        "test": "jest",
        "lint": "eslint",
        "prepare": "yarn build",
        "prettier": "prettier  --config prettier.config.js --write ./src/",
        "start-demo": "cd ./examples/demo && yarn dev",
        "build": "vite build && tsc",
        "build-non-vite": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
        "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
        "watch": "tsc --outDir dist/esm --module es2015 --watch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ValentinnDimitroff/ra-auth-ui.git"
    },
    "author": "Valentin Dimitroff",
    "license": "MIT"
}
